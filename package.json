    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-universe": "^15.0.3",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.14",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "supabase": "^2.65.6",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
